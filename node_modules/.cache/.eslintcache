\n  useEffect(() => {\n    const endpoint =\n      value === \"\"\n        ? upcoming\n        : searching\n\n    setLoading(true)\n    setDisabled(true)\n    fetch(endpoint)\n      .then((response) => response.json())\n      .then((json) => {\n\n        setData((previous) =>\n          currentPage === 1 ? json.results : [...previous, ...json.results]\n        );\n\n        setCounts({\n          total_pages: json.total_pages,\n          total_results: json.total_results\n        });\n        setDisabled(false)\n      })\n      .catch((error) => console.error(\"Error:\", error))\n      .finally(() => setLoading(false))\n\n  }, [value, currentPage, searching, upcoming])\n\n  return (\n    <>\n      <Results>\n        {value ? `Resultados para: ${value}` : \"Últimos lançamentos\"}\n      </Results>\n\n      <Box>\n        {\n          data?.map(movies => (\n            <ListMovies {...movies} key={movies.id} />\n          ))\n        }\n      </Box>\n\n      <Box>\n        {loading && <Loading />}\n      </Box>\n\n      <Box>\n        {hasPage ? (\n          <Button onClick={loadMoreItems} disabled={disabled}>\n            Load More\n          </Button>\n        ) : (\n          <Results>\n            Mostrando {counts.total_results} de {counts.total_results} Movies\n          </Results>\n        )}\n      </Box>\n    </>\n  )\n}\n\nexport default Movies","/home/paulinho/Documentos/projectest/my-app/src/components/Wrapper/style.js",[],[],"/home/paulinho/Documentos/projectest/my-app/src/components/Header/style.js",[],[],"/home/paulinho/Documentos/projectest/my-app/src/Pages/Movies/ListMovies.tsx",["124","125"],[],"import { ReactHTMLElement, ReactNode } from \"react\"\nimport Card from \"../../components/Card\"\n\nimport { IMoviesProps } from '../../interfaces/MoviesProps'\n\nconst ListMovies: React.FC<IMoviesProps> = ({\n  poster_path,\n  id,\n  title,\n  release_date,\n  overview }) => {\n\n  return (\n    <>\n      <Card>\n        {poster_path ? (\n          <img src